{
    "version": 2,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.3"
        }
    },
"routes": [
    {
        "src": "/storage/images/(.*)",
        "dest": "/public/storage/images/$1"
    },
    {
        "src": "/images/(.*)",
        "dest": "/public/images/$1"
    },
    {
        "src": "/build/(.*)",
        "dest": "/public/build/$1"
    },
    {
        "src": "/(.*)",
        "dest": "/api/index.php"
    }
],
    "env": {
        "APP_ENV": "development",
